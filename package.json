{
  "version": "0.0.0-development",
  "name": "@telus/remark-preset-lint-markdown",
  "description": "remark preset to configure remark-lint with rules that enforce the markdown style guide",
  "author": "Ahmad Nassri <ahmad.nassri@telus.com> (https://telus.com/digital/)",
  "homepage": "https://github.com/telus/remark-preset-lint-markdown",
  "repository": {
    "type": "git",
    "url": "https://github.com/telus/remark-preset-lint-markdown.git"
  },
  "license": "MIT",
  "keywords": [
    "remark",
    "preset",
    "markdown",
    "style",
    "guide"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/telus/remark-preset-lint-markdown/issues"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "npm run lint:javascript && npm run lint:markdown && npm run lint:editorconfig",
    "lint:javascript": "eslint .",
    "lint:markdown": "remark --quiet --frail .",
    "lint:editorconfig": "editorconfig-checker ."
  },
  "dependencies": {
    "remark-preset-lint-markdown-style-guide": "^2.1.2"
  },
  "devDependencies": {
    "@telusdigital/eslint-config": "^1.2.0",
    "editorconfig-checker": "^1.3.3",
    "eslint": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "remark-cli": "^5.0.0"
  }
}
